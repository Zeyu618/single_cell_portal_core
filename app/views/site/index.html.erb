<%= render partial: 'main_banner_content' %>
<div class="container-fluid home-page-fix" id="wrap">
  <div class="row section-pad" id="main-body">
    <div class="col-md-12">

      <% if @selected_branding_group.nil? %>
        <div class="home-nav" id="collections-tab">
          <a href="/single_cell/collections" class="btn btn-primary" data-toggle="tooltip" title="Groups of studies with a common research theme">
            Browse collections
          </a>
        </div>
      <% end %>
      <div id="home-page-content"></div>
      <script type="text/javascript" nonce="<%= content_security_policy_script_nonce %>">
        window.SCP.renderComponent('home-page-content', 'HomePageContent', {})
      </script>
    </div>
  </div>
</div>
