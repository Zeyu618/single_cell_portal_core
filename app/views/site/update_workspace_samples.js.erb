closeModalSpinner('#generic-modal-spinner', "#generic-modal", function() {
    $("#workflow_inputs_samples").html("<%= escape_javascript(select_tag :workflow_samples, options_for_select(@samples), {multiple: true, class: 'form-control', size: 10 }) %>")
    if ( <%= @empty_samples_table == true %>) {
        updateSampleTable([]);
    }
    $('#workflow_study_data').val('')
    showMessageModal("<%= @notice.present? ? @notice.html_safe : nil %>", "<%= @alert.present? ? @alert.html_safe : nil %>");
});
