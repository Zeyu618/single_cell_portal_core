<div class="form-group row">
  <div class="col-sm-6">
    <%= f.fields_for :options do |opts| %>
      <%= opts.label :cluster_file_id, 'Cluster Target' %>&nbsp;<span class="fas fa-question-circle" data-toggle="tooltip" data-placement="right" title="Select which Cluster file these labels pair with.  You cannot save this file until this is set."></span> <br />
      <%= opts.select :cluster_file_id, options_for_select([]), {include_blank: 'Please select a Cluster file...'}, class: 'form-control cluster-file-select bundle-select' %>
    <% end %>
  </div>
</div>
